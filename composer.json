{
    "name": "stavarengo/travel-sorter",
    "description": "Travel sorter.",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TravelSorter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TravelSorter\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "php-di/php-di": "^6.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:4000 -t public/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-html ./test-coverage"
    }
}
